{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "api": [
    {
      "name": "Bingsearch",
      "url": "http://localhost:3400/api/v2",
      "description": "Bingsearch——API",
      "parameters": {
        "properties": {
          "query": {
            "description": "搜索的文本内容",
            "type": "string"
          }
        },
        "required": ["query"],
        "type": "object"
      }
    }
  ],
  "author": "ZWL_FineHow",
  "createdAt": "2024-11-21",
  "gateway": "http://localhost:3400/api/gateway",
  "homepage": "https://github.com/FineHow/Bingsearch-Plugin",
  "settings": {
    "type": "object",
    "required": ["BING_API_KEY"],
    "properties": {
      "BING_API_KEY": {
        "title": "Bing API Key",
        "description": "we use [Bing_API]as backend service | 该插件使用 Bing_API 作为搜索服务",
        "type": "string",
        "minLength": 32,
        "maxLength": 32,
        "format": "password"
      }
    }
  },
  "identifier": "Bingsearch-identifier",
  "meta": {
    "avatar": "🚀",
    "tags": ["Bingsearch"],
    "title": "Bing联网搜索",
    "description": "基于bing搜索引擎的互联网搜索助手"
  },
  "ui": {
    "url": "http://localhost:3400",
    "height": 200
  },
  "version": "1"
}
